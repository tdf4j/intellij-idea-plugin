<idea-plugin>
  <id>org.tdf4j.plugin</id>
  <name>tdf4j</name>
  <vendor>tdf4j.org</vendor>

  <description><![CDATA[
    Enter short description for your plugin here.<br>
    <em>most HTML tags may be used</em>
    ]]></description>

  <extensions defaultExtensionNs="com.intellij">
    <fileTypeFactory implementation="org.tdf4j.plugin.TDFFileTypeFactory"/>
    <lang.parserDefinition language="Tdf" implementationClass="org.tdf4j.plugin.TDFParserDefinition"/>
    <lang.syntaxHighlighterFactory key="Tdf" implementationClass="org.tdf4j.plugin.TDFSyntaxHighlighterFactory"/>
    <colorSettingsPage implementation="org.tdf4j.plugin.TDFColorSettingsPage"/>
  </extensions>
</idea-plugin>